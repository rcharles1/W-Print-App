<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="historyRowModel" type="com.example.wprintapp.modules.history.data.model.HistoryRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearPrintHistoryC"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
        <LinearLayout android:id="@+id/linearRowmusic"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_weight="0.86" android:layout_width="0dp" android:orientation="horizontal">
            <ImageButton
                android:background="@drawable/rectangle_border_bluegray_101_radius_10"
                android:id="@+id/btnMusic"
                android:layout_height="@dimen/_60pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="@dimen/_60pxh"
                android:paddingHorizontal="@dimen/_14pxh"
                android:paddingVertical="@dimen/_14pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_music" tools:ignore="ContentDescription"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearColumntestpagepdf"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_weight="0.68" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtTestPagepdf"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{historyRowModel.txtTestPagepdf}"
                    style="@style/txtMontserratromanmedium18" tools:text="@string/lbl_test_page_pdf"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowprinter"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imagePrinter"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_printer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_printer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCanonPIXMA"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{historyRowModel.txtCanonPIXMA}"
                        style="@style/txtMontserratromanlight13_1" tools:text="@string/lbl_canon_pixma"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowsettings"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNearbyPrinting"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.89"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{historyRowModel.txtNearbyPrinting}"
                        style="@style/txtMontserratromanlight11" tools:text="@string/lbl_nearby_printing"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcalendar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalendar"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_width="@dimen/_11pxh"

                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1052023"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{historyRowModel.txt1052023}"
                        style="@style/txtMontserratromanlight11" tools:text="@string/lbl_10_5_2023"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageCheckmark"
            android:layout_height="@dimen/_31pxh"
            android:layout_marginBottom="@dimen/_45pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_55pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="@dimen/_33pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark_green_a700"
            tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_green_a700"/>
    </LinearLayout>
</layout>
